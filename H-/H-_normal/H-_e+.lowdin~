
GEOMETRY
        e-(H) 6-311++G.d.p 0 0 0 addParticles=1
	e+    GRIBAKIN10S4P2D
        H     dirac	   0 0 0
END GEOMETRY

TASKS
        method = "RHF"
END TASKS

CONTROL
	units="BOHRS"
readCoefficients=F
convergenceMethod=1
	overlapEigenThreshold=1E-6
	readCoefficients=T
	scfElectronicMaxIterations=0
	HFprintEigenvalues=T
	totalEnergyTolerance = 1E-9
END CONTROL

