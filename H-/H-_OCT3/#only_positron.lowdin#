END TASKS

CONTROL
	units="BOHRS"
	readCoefficients=T
	convergenceMethod=1
	overlapEigenThreshold=1E-6
	readCoefficients=T
	freezeElectronicOrbitals=T
	HFprintEigenvalues=T
	totalEnergyTolerance = 1E-9
END CONTROL

